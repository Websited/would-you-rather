{
  "printWidth": 80,
  "tabWidth": 4,
  "newline-before-return": true,
  "no-duplicate-variable": [true, "check-parameters"],
  "no-var-keyword": true,
  "endOfLine": "lf",
  "semi": true,
  "singleQuote": true,
  "bracketSpacing": true,
  "quoteProps": "as-needed",
  "trailingComma": "es5",
  "arrowParens": "always",
  "jsxBracketSameLine": false,
  "options": {
      "tabWidth": 4
  }
}

